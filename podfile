# platform :ios, '9.0'

target 'swift_FireBase' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for swift_FireBase
	pod 'Firebase/Core'
	pod 'Firebase/Auth'
	pod 'GoogleSignIn'
end
